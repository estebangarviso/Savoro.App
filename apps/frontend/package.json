{
  "name": "@savoro/frontend",
  "version": "1.0.0",
  "description": "Vite frontend build system for Savoro restaurant management system",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "clean": "rm -rf staticfiles/.vite staticfiles/manifest.json staticfiles/**/chunks/ node_modules/.vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,css,html,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,css,html,json,md}\""
  },
  "dependencies": {
    "materialize-css": "^1.0.0"
  },
  "devDependencies": {
    "@types/materialize-css": "1.0.14",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-regexp": "2.10.0",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "16.5.0",
    "prettier": "3.6.2",
    "rollup-plugin-copy": "3.5.0",
    "terser": "5.44.1",
    "vite": "5.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
